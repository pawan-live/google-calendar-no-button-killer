{
  "manifest_version": 3,
  "name": "Disable NO Button",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://calendar.google.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icons/disabled.png"
    }
  }
}
